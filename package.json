{
	"name": 		"nearley",
	"displayName": 	"Nearley",
	"description": 	"Nearley support for Visual Studio Code",
	"icon": 		"logo.png",
	"version":		"1.0.1",
	"publisher": 	"karyfoundation",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:nearley"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [{
			"id": 				"nearley",
			"extensions": 		[ ".ne" ],
			"aliases": 			[ "Nearley", "nearley" ],
			"filenames": 		[ ".ne" ],
			"configuration": 	"./language-configuration.json"
		}],
		"grammars": [{
			"language": 	"nearley",
			"scopeName": 	"source.nearley",
			"path": 		"./syntaxes/nearley.tmLanguage.json",
			"embeddedLanguages": {
				"source.js": 	"javascript"
			}
		}]
	},
	"devDependencies": {
		"vscode": "0.10.x"
	}
}